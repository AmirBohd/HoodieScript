{
    "name": "hoodiescript-vscode",
    "displayName": "HoodieScript-vscode",
    "description": "Visual Studio Code extension for HoodieScript",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Snippets"
    ],
    "activationEvents": [
        "onLanguage:lua"
    ],
    "console": "integratedTerminal",
    "main": "main.js",
    "contributes": {
        "snippets": [
            {
                "language": "lua",
                "path": "./snippets/snippets.json"
            }
        ]
    }
}
